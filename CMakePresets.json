{
    "version": 8,
    "configurePresets": [
        {
            "name": "win-clangd-debug",
            "displayName": "Clang 19.1.7 x86_64-w64-windows-gnu (mingw64)",
            "description": "using complie C = C:\\msys64\\mingw64\\bin\\clang.exe, CXX = C:\\msys64\\mingw64\\bin\\clang++.exe",
            "generator": "Ninja",
            "binaryDir": "${sourceDir}/build/${presetName}",
            "cacheVariables": {
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/out/install/${presetName}",
                "CMAKE_C_COMPILER": "C:/msys64/mingw64/bin/clang.exe",
                "CMAKE_CXX_COMPILER": "C:/msys64/mingw64/bin/clang++.exe",
                "CMAKE_BUILD_TYPE": "Debug"
            }
        }
    ]
}